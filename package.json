{
  "version": "0.0.1",
  "license": "MIT",
  "name": "jbrowse-plugin-gwas",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "config": {
    "port": 9000,
    "browse": {
      "port": 8999
    },
    "jbrowse": {
      "plugin": {
        "name": "GWAS"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all --sequential clean --parallel start:*",
    "start:watch": "cross-env JB_NPM=false NODE_ENV=development rollup --config --watch --bundleConfigAsCjs",
    "start:server": "serve --cors --listen $npm_package_config_port .",
    "prebuild": "npm-run-all clean",
    "build": "rollup --config --bundleConfigAsCjs",
    "browse": "npm-run-all jbrowse:*",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "npm-run-all test",
    "prepack": "npm-run-all build",
    "postversion": "git push --follow-tags"
  },
  "jbrowse-plugin": {
    "name": "GWAS"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.10.4",
    "@emotion/react": "^11.10.4",
    "@eslint/js": "^9.15.0",
    "@jbrowse/cli": "^2.6.1",
    "@jbrowse/core": "^2.6.1",
    "@jbrowse/development-tools": "^2.1.1",
    "@jbrowse/plugin-linear-genome-view": "^2.17.0",
    "@jbrowse/plugin-wiggle": "^2.17.0",
    "@mui/material": "^6.1.4",
    "@mui/system": "^6.1.4",
    "@mui/x-data-grid": "^7.6.1",
    "@schemastore/package": "^0.0.10",
    "@types/node": "^22.7.6",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "mobx": "^6.0.0",
    "mobx-react": "^9.1.0",
    "mobx-state-tree": "5.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^3.0.0",
    "rxjs": "^7.8.1",
    "serve": "^14.2.0",
    "shx": "^0.3.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.3.0",
    "tss-react": "^4.8.6",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.16.0"
  },
  "private": true
}
